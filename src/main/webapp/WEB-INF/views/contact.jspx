    <!-- Modal -->



    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header" style = "background: #ffbc00">
                    <h5 class="modal-title" id="exampleModalLabel" style = "color: black"><b>To. Soft Plus Technology</b></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                             <div class="row">
                                <div class="col-sm-12">
                                    <div class="col-sm-2">
                                        <label for="name" class="col-form-label">Name: <span style="color: red">*</span></label>
                                    </div>
                                    <div class="col-sm-10">
                                         <input type="text" class="form-control" placeholder="Your name..." id="inputname"/>
                                    </div>
                                </div>
                             </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="col-sm-2">
                                        <label for="email"  class="col-form-label">Email: <span style="color: red">*</span></label>
                                    </div>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" placeholder="Your email..." id="inputemail"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <p style="color: red"></p>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-12">
                                     <div class="col-sm-2">
                                         <label for="exampleInputFile">Resume : </label>
                                     </div>
                                    <div class="col-sm-10">
                                          <input type="file" class="form-control-file" id="inputfile" aria-describedby="fileHelp"/>
                                          <small id="fileHelp" class="form-text text-muted"></small>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="form-group">
                             <div class="row">
                                 <div class="col-sm-12">
                                     <div class="col-sm-2">
                                        <label for="message-text" class="col-form-label">Message:</label>
                                     </div>
                                     <div class="col-sm-10">

                                        <textarea  placeholder="Comments..." style="width: 100%" id="inputmessage"><jsp:text/>
                                        </textarea>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer" style = "background: #ffbc00">
                    <button type="button" class="btn btn-danger" data-dismiss="modal" id="close_modal">Close</button>
                    <button type="button" class="btn btn-primary" data-dismiss="modal" id="bnsend">Send message</button>
